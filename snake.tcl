proc load_one_image {n s f b} {
  set v "i-$s.xbm"
  set vv "i-$s-m.xbm"
  global $v $vv
  image create bitmap "image_$n" \
       -data [set $v] -maskdata [set $vv] \
       -foreground "#$f" -background "#$b"
}

proc load_images {} {
  foreach {nm fg bg} {poisoccustop       66cc66 666666
                      voidvoidvoid       ffffff ffffff } {
    load_one_image $nm $nm $fg $bg
  }
  foreach {nm} {0 1 2 3} {
    load_one_image "voidvoidfire$nm" "voidvoidfire$nm" ff0000 ffff00
  }
  foreach {nm} {0 1 2 3} {
    set p [expr {3-$nm}]
    load_one_image "voidvoidfireboom$p" "voidvoidfireboom$nm" {ff0000} {ffff00}
  }
  foreach {nm fg bg} {0 333333 999999
		      1 660000 ffcccc
		      2 990099 ff99ff
		      3 006600 00ff00
		      4 333333 ffcccc
		      5 9999ff ffffff
		      6 666600 ffcc00
		      7 333300 999900
		      8 6666ff 6666ff
		      9 000000 ff0000} {
    load_one_image "foodoccufood$nm" "foodoccufood$nm" $fg $bg
  }
  foreach {nm} {0 1 2 3} {
    load_one_image "foodoccufoodinit$nm" "foodoccufoodinit$nm" {000000} {00ffff}
  }
  # части тела
  foreach {n m} {0 0 0 1
                 1 1 1 2
		 2 1 2 3} {
    load_one_image "poisoccukillbody$n$m" "poisoccukillbody$n$m" 665200 ffb400
    set a [expr {($n+2)%4}]
    set b [expr {($m+2)%4}]
    load_one_image "poisoccukillbody$b$a" "poisoccukillbody$n$m" 665200 ffb400
  }
  foreach {n} {0 1 2 3} {
    load_one_image "poisoccukillhead$n" "poisoccukillhead$n" 665200 ffb400
  }
  foreach {nm} {00 01    03
                10 11 12
		   21 22 23
		30    32 33} {
    load_one_image "poisoccukilltail$nm" "poisoccukilltail$nm" 665200 ffb400
  }
}

proc configure_top_menu {} {
  set m [menu .root_menu -tearoff 0]
  set g [menu $m.game -tearoff 0]
  set s [menu $m.setup -tearoff 0]
  set h [menu $m.help -tearoff 0]
  set sp [menu $s.sp -tearoff 0]
  set sg [menu $s.gf -tearoff 0]
  set sz [menu $s.sz -tearoff 0]
  $m add cascade -label {Game} -menu $g
  $m add cascade -label {Options} -menu $s
  $m add cascade -label {Help} -menu $h
  foreach {l a c} {{New game}      n menu_start
                   {Pause/unpause} p menu_pause
		   {Stop game}     s stop_times
		   {Exit}          q exit} {
    $g add command -label $l -accelerator $a -command $c
    bind . <Key-$a> $c
  }
  $g insert 3 separator
  $s add cascade -label {Speed} -menu $sp
  $s add cascade -label {Gifts (restart)} -menu $sg
  $s add cascade -label {Size (restart)} -menu $sz
  foreach {l a v} {slowest 1 800
                   slow    2 500
		   normal  3 400
		   fast    4 300
		   fastest 5 200} {
    $sp add radiobutton -label $l -accelerator $a \
                        -variable snake_speed -value $v
    bind . <Key-$a> "set snake_speed $v"
  }
  for {set l 1} {$l<21} {incr l} {
    $sg add radiobutton -label $l \
                        -columnbreak [expr {($l-1)%5 == 0 && $l != 1}] \
                        -variable snake_gifts -value $l \
			-command menu_start
  }
  foreach {aw ah} {14 10 20 14 30 20 40 30} {
    $sz add radiobutton -label "${aw}x$ah" -command [list menu_resize $aw $ah]
  }
  foreach {l b} {Rules {You must guide your snake\
across the board and feed or fire it any gifts.\
The snake becomes longer as it eats, so you must\
keep it from hitting it own tail and the edges of the board.}
                 Control {Use arrow keys and space to contorl your snake}
                 About   {The Snake Game
v. 4.4
Copyright (c) 2006 Michurin Alexey
This program is free software; you can redistribute it and/or\
modify it under the terms of the GNU General Public License.
This program is distributed in the hope that it will be useful, but\
WITHOUT ANY WARRANTY.}
                } {
    $h add command -label $l -command [list help_window $l $b]
  }
  return $m
}

proc configure_window {} {
  . configure -menu [configure_top_menu]
  wm resizable . 0 0
  wm title . Snake
  pack [canvas .area -width 0 -height 0 -bg {#d5ffa8} -cursor tcross \
                     -insertborderwidth 0 -bd 0 -selectborderwidth 0 \
		     -highlightthickness 0]
  bind . <Key-Right> {set snake_dir 0}
  bind . <Key-Down>  {set snake_dir 1}
  bind . <Key-Left>  {set snake_dir 2}
  bind . <Key-Up>    {set snake_dir 3}
  bind . <Key-space> {init_fire}
  focus .
}

proc create_screen {} {
  global unit_size area_width area_height
  set w [expr {$unit_size*$area_width}]
  set h [expr {$unit_size*$area_height}]
  .area configure -width $w -height $h
  for {set x 0} {$x<$area_width} {incr x} {
    for {set y 0} {$y<$area_height} {incr y} {
      set x1 [expr {$x*$unit_size}]
      set y1 [expr {$y*$unit_size}]
      .area create image $x1 $y1 -anchor nw -tag "$x-$y"
    }
  }
}

proc set_image {x y n} {
  .area itemconfigure "$x-$y" -image "image_$n"
}

proc get_image {x y} {
  return [string range [.area itemcget "$x-$y" -image] 6 end]
}

proc init_screen {} {
  global area_width area_height
  set h [expr {$area_height - 1}]
  set w [expr {$area_width  - 1}]
  for {set i 0} {$i<$area_width} {incr i} {
    set_image $i  0 poisoccustop
    set_image $i $h poisoccustop
  }
  for {set j 1} {$j<$h} {incr j} {
    set_image  0 $j poisoccustop
    set_image $w $j poisoccustop
  }
  for {set i 1} {$i<$w} {incr i} {
    for {set j 1} {$j<$h} {incr j} {
      set_image  $i $j voidvoidvoid
    }
  }
}

proc init_data {} {
  global snake_body snake_dir snake_length
  set snake_dir 0
  set snake_length 3
  set snake_body [list]
  for {set x 1} {$x<4} {incr x} {
    set b poisoccukillbody00
    if {$x == 1} {set b poisoccukilltail00}
    if {$x == 3} {set b poisoccukillhead0}
    set_image $x 1 $b
    lappend snake_body [list $x 1 $snake_dir]
  }
}

proc eat_init {x y n} {
  if {$n} {
    if {[string range [get_image $x $y] 0 11] != {foodoccufood}} return
  }
  if {$n<4} {
    set_image $x $y "foodoccufoodinit$n"
    incr n
    after 70 eat_init $x $y $n
  } else {
    set m [expr {int(rand()*10)}]
    set_image $x $y "foodoccufood$m"
  }
}

proc init_one_eat {} {
  global area_width area_height snake_gifts
  set w [expr {$area_width-1}]
  set h [expr {$area_height-1}]
  set o [list]
  set c 0
  for {set x 1} {$x<$w} {incr x} {
    for {set y 1} {$y<$h} {incr y} {
      set im [get_image $x $y]
      if {[string range $im 4 7] == {void}} {
        lappend o [list $x $y]
      }
      if {[string range $im 0 3] != {pois}} {incr c}
    }
  }
  if {[llength $o]} {
    foreach {x y} [lindex $o [expr {int(rand()*[llength $o])}]] {}
    eat_init $x $y 0
  }
#  puts $c
  return $c
#  return [expr {$c != ($area_width-2)*($area_height-2)}]
}

proc init_eat {} {
  global snake_gifts
  for {set i 0} {$i<$snake_gifts} {incr i} {init_one_eat}
}

proc one_step {} {
  global snake_body snake_dir snake_length snake_speed
  # двигаем голову
  foreach {hx hy hd} [lindex $snake_body end] {}
  if {($hd == 0 && $snake_dir != 2) ||
      ($hd == 1 && $snake_dir != 3) ||
      ($hd == 2 && $snake_dir != 0) ||
      ($hd == 3 && $snake_dir != 1)} {
    set_image $hx $hy "poisoccukillbody$hd$snake_dir"
    incr hx [lindex {1 0 -1  0} $snake_dir]
    incr hy [lindex {0 1  0 -1} $snake_dir]
    set img [string range [get_image $hx $hy] 0 3]
    lappend snake_body [list $hx $hy $snake_dir]
    set_image $hx $hy "poisoccukillhead$snake_dir"
    if {$img == {food}} {
      incr snake_length 2
      if {![init_one_eat]} {
        stop_timers
        return
      }
    }
    if {$img == {pois}} {
      stop_timers
      return
    }
    # двигаем хвост
    if {[llength $snake_body] > $snake_length} {
      foreach {ax ay ad} [lindex $snake_body 0] {}
      set snake_body [lreplace $snake_body 0 0]
      foreach {bx by bd} [lindex $snake_body 0] {}
      set cd [lindex [lindex $snake_body 1] 2]
      set_image $ax $ay {voidvoidvoid}
      set_image $bx $by "poisoccukilltail$bd$cd"
    }
  }
  after $snake_speed one_step
}

proc stop_timers {} {
  foreach {id} [after info] {after cancel $id}
}

proc menu_start {} {
  stop_timers
  init_screen
  init_data
  init_eat
  one_step
}

proc menu_pause {} {
  global snake_dir
  set snake_dir [expr {($snake_dir+2)%4}]
}

proc menu_resize {w h} {
  global area_width area_height
  stop_timers
  .area delete all
  set area_width  $w
  set area_height $h
  create_screen
  init_screen
  init_data
  init_eat
  one_step
}

proc help_window {t m} {
  if {[winfo exists .help]} {destroy .help}
  toplevel .help
  pack [label .help.title -text $t -relief raised] -fill x
  pack [message .help.body -text $m -aspect 300 -justify center]
  pack [button .help.ok -text ok -command {destroy .help}] -fill x
  wm resizable .help 0 0
  wm title .help $t
  bind .help <Key-q> {destroy .help}
}

# змемея не чувствует снарядик(и), но они могут ранить её
# не надо учитывать огонь и при постановке новой еды
proc init_fire {} {
  global snake_body
  foreach {hx hy hd} [lindex $snake_body end] {}
    # расшифровку направления в отдельную функцию??
#  set dx [lindex {1 0 -1  0} $hd]
#  set dy [lindex {0 1  0 -1} $hd]
  fire_step $hx $hy $hd
}

proc fire_step {x y d} {
  global snake_length
  set im [string range [get_image $x $y] 8 11]
  if {$im == {fire}} {set_image $x $y voidvoidvoid}
    # расшифровку направления в отдельную функцию??
  incr x [lindex {1 0 -1  0} $d]
  incr y [lindex {0 1  0 -1} $d]
  set im [string range [get_image $x $y] 8 11]
#  puts $im
  if {$im == {stop} || $im == {fire}} return
  if {$im == {food}} {
    # проглатывание надо вынести в отдельную функцию
    init_one_eat
    incr snake_length 2
    fire_one $x $y 0
    return
  }
  set_image $x $y "voidvoidfire$d"
  if {$im == {kill}} {
    stop_timers
    return
  }
  after 70 [list fire_step $x $y $d]
}

proc fire_one {x y n} {
  set im [string range [get_image $x $y] 0 11]
  if {$n} {
    if {$im != {voidvoidfire}} return
  } else {
    if {$im != {foodoccufood}} return
  }
  if {$n < 4} {
    set_image $x $y "voidvoidfireboom$n"
    incr n
    after 70 [list fire_one $x $y $n]
  } else {
    set_image $x $y voidvoidvoid
  }
}

